{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off", // ignores console.log formatting
        "linebreak-style": "off", // ignores LF vs CRLF errors
        "no-alert": "off"
    }
}
